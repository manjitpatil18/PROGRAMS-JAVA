# PROGRAMS-JAVA
public class Main
{
	public static void main(String[] args)
	{
            String val = "abc";
            int[] arrr = new int[val.length()];
            int b = 0;
            for (int i = 0; i < val.length(); i++)
            {
                char ch=val.charAt(i);
                b+=(int)ch;
            }
               System.out.println(b);
	}
}
